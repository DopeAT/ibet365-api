{
  "kind": "collectionType",
  "collectionName": "challenges",
  "info": {
    "singularName": "challenge",
    "pluralName": "challenges",
    "displayName": "Challenge",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "totalBets": {
      "type": "integer"
    },
    "challengeStatus": {
      "type": "enumeration",
      "enum": [
        "Void",
        "In progress",
        "Win",
        "Lose"
      ]
    },
    "bank": {
      "type": "decimal"
    },
    "bets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bet.bet"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}

{
  "kind": "collectionType",
  "collectionName": "bet_categories",
  "info": {
    "singularName": "bet-category",
    "pluralName": "bet-categories",
    "displayName": "BetCategory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "betStatus": {
      "type": "enumeration",
      "enum": [
        "In progress",
        "Win",
        "Lose",
        "Void"
      ]
    },
    "bets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bet.bet"
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
